{
  "name": "nodejs-server-side-boilerplate",
  "version": "0.0.1",
  "description": "node.js server side starter boilerplate",
  "scripts": {
    "start": "cross-env NODE_ENV=development nodemon --watch src --ext ts,js --exec ts-node ./src/index.ts",
    "build": "cross-env NODE_ENV=production tsc",
    "clean": "rimraf ./build",
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "test:staged": "cross-env NODE_ENV=test jest --findRelatedTests",
    "test:coverage": "cross-env NODE_ENV=test jest --coverage",
    "lint": "eslint './**/*.[jt]s'",
    "lint:fix": "eslint './**/*.[jt]s' --fix",
    "commit": "git cz"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cyaenorama/nodejs-server-side-boilerplate.git"
  },
  "keywords": [],
  "author": "Sayan Das",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cyaenorama/nodejs-server-side-boilerplate/issues"
  },
  "homepage": "https://github.com/cyaenorama/nodejs-server-side-boilerplate#readme",
  "dependencies": {
    "@babel/runtime": "^7.7.4",
    "axios": "^0.21.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "fs-extra": "^8.1.0",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.7.4",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-decorators": "^7.7.4",
    "@babel/plugin-proposal-export-namespace-from": "^7.7.4",
    "@babel/plugin-proposal-function-sent": "^7.7.4",
    "@babel/plugin-proposal-json-strings": "^7.7.4",
    "@babel/plugin-proposal-numeric-separator": "^7.7.4",
    "@babel/plugin-proposal-throw-expressions": "^7.7.4",
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@babel/plugin-syntax-import-meta": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.2",
    "@types/fs-extra": "^8.0.1",
    "@types/jest": "^24.0.23",
    "@types/node": "^12.12.14",
    "@typescript-eslint/eslint-plugin": "^2.9.0",
    "@typescript-eslint/parser": "^2.9.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "commitizen": "^4.0.3",
    "cross-env": "^6.0.3",
    "cz-conventional-changelog": "3.0.2",
    "eslint": "^6.7.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^23.0.4",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-unicorn": "^13.0.0",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "lint-staged": "^9.4.3",
    "nodemon": "^2.0.1",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0",
    "ts-jest": "^24.2.0",
    "ts-node": "^8.5.2",
    "typescript": "^3.7.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
